<?xml version="1.0"?>
<registry>

  <!-- Initialize controlpanel configuration in registry -->
  <records interface="eea.annotator.controlpanel.interfaces.ISettings" />

  <!-- Enable resource dependencies in "plone-legacy" bundle for Plone 5 -->
  <records prefix="plone.bundles/plone-legacy"
            interface="Products.CMFPlone.interfaces.IBundleRegistry">
    <value key="resources" purge="false">
      <element>resource-plone-app-jquerytools-js</element>
      <element>annotator</element>
    </value>
  </records>

</registry>
